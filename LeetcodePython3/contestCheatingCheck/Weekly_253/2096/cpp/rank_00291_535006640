class Solution {
public:
    vector<int> longestObstacleCourseAtEachPosition(vector<int>& o) {
        vector<int> dp(o.size()+1,INT_MAX),ret(o.size());
        for(int i=0;i<o.size();i++)
        {
            ret[i]=upper_bound(dp.begin(),dp.end(),o[i])-dp.begin()+1;
            dp[ret[i]-1]=o[i];
        }
        return ret;
    }
};