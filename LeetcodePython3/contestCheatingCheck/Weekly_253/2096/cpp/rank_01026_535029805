class Solution {
public:
    vector<int> longestObstacleCourseAtEachPosition(vector<int>& o) {
        vector<int> LIS;
        vector<int> dp(o.size());
        for(int i=0;i<o.size();i++){
            int position= upper_bound(LIS.begin(), LIS.end(), o[i]) - LIS.begin();
            if(position== LIS.size()) LIS.push_back(o[i]);
            else LIS[position]= o[i];
            dp[i]= position+1;
        }
        return dp;
    }
};