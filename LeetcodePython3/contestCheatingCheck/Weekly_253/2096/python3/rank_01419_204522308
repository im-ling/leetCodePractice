class Solution:
    def longestObstacleCourseAtEachPosition(self, nums: List[int]) -> List[int]:
        tails, res = [0] * len(nums), 0
        ans = []
        for num in nums:
            i, j = 0, res
            while i < j:
                m = (i + j) // 2
                if tails[m] <= num: i = m + 1
                else: j = m
            tails[i] = num
            if j == res:
                res += 1
            ans.append(i+1)
        return ans