class Solution:
    def longestObstacleCourseAtEachPosition(self, obstacles: List[int]) -> List[int]:
        res = [1]
        lis = [obstacles[0]]
        for i in range(1, len(obstacles)):
            if obstacles[i] >= lis[-1]:
                lis.append(obstacles[i])
                res.append(len(lis))
            else:
                j = bisect_right(lis, obstacles[i])
                lis[j] = obstacles[i]
                res.append(j+1)
        return res