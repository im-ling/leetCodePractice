class Solution:
    def lengthOfLIS(self, nums):
        tails = [0] * len(nums)
        ret = []
        size = 0
        for x in nums:
            i, j = 0, size
            while i != j:
                m = (i + j) // 2
                if tails[m] <= x:
                    i = m + 1
                else:
                    j = m
            tails[i] = x
            size = max(i + 1, size)
            ret.append(i + 1)
        return ret
    def longestObstacleCourseAtEachPosition(self, ob: List[int]) -> List[int]:
        return self.lengthOfLIS(ob)